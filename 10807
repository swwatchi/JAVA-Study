package JavaStudy;

import java.util.Scanner;

class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int answer = 0;
		int[] arr = new int[a];
	
		for(int i=0; i<a;i++) {
			arr[i] = sc.nextInt();
		}
		
		int V = sc.nextInt();

		
		for(int j =0; j<arr.length;j++) {
			if(arr[j] == V) {
				answer++;
			}
			
		}
		System.out.println(answer);
	}
}
