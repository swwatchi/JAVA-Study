package JavaStudy;

import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        String b = sc.next();
        String[] arr = b.split(""); //split("") ""을 기준으로 분리해서 배열에 삽입
        int answer = 0;
        for(int i=0;i<a;i++) {
        	answer += Integer.parseInt(arr[i]); //Integer.parseInt()로 int형으로 변환
        }
        System.out.println(answer);
    }
}
