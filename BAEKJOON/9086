package JavaStudy;

import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        
        for(int i=0;i<a;i++) {
        	String word = sc.next();
        	System.out.println(word.charAt(0)+ ""+word.charAt(word.length()-1)); //charAt(0)은 0번째 문자를 출력 length()-1은 마지막 인덱스를 출력
        }
    }
}
