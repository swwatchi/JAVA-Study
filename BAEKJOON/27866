package JavaStudy;

import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String a = sc.next();
        int b = sc.nextInt();
        
        System.out.println(a.charAt(b-1)); //a의 charAt으로 n번 째의 값을 출력, 0부터 시작하기 때문에 -1을 해줌 
    }
}
